@model BW_Admin.tbl_Tour

@{
    ViewBag.Title = "Details";
}

<div class="content">
    <div class="module">
        <div class="module-head">    <h3>Tour</h3>    </div>   <div class="module-body">

            <div>

                <h5>
                    Name
                </h5>

                <div class="well">
                    @Html.Raw(Model.tor_Name)
                </div>

                <h5>
                    Overview
                </h5>

                <div class="well">
                    @Html.Raw(Model.tor_Overview)
                </div>

                <h5>
                    Description
                </h5>

                <div class="well">
                    @Html.Raw(Model.tor_Description)
                </div>

                <h5>
                    Priority
                </h5>

                <div class="well">
                    @Html.Raw(Model.tor_Priority)
                </div>

                <h5>
                    Active
                </h5>

                <div class="well">
                    @Html.Raw(Model.tor_Active)
                </div>

                <h5>
                    Banner Image
                </h5>

                <div class="well">
                    <img class="img-responsive" src="~/Pictures/@Model.tor_BannerImage" />
                </div>

                <h5>
                    Square Image
                </h5>

                <div class="well">
                    <img class="img-responsive" src="~/Pictures/@Model.tor_SquareImage" />
                </div>

                <h5>
                    Background Image 1
                </h5>

                <div class="well">
                    <img class="img-responsive" src="~/Pictures/@Model.tor_BackgroundImage1" />
                </div>

                <h5>
                    Background Image 2
                </h5>

                <div class="well">
                    <img class="img-responsive" src="~/Pictures/@Model.tor_BackgroundImage2" />
                </div>

            </div>
            <p>
                @Html.ActionLink("Edit", "Edit", new { id = Model.tor_ID }) |
                @Html.ActionLink("Back to List", "Index")
            </p>
        </div>
    </div>
</div>
